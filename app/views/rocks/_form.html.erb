<%= form_for @rock do |f| %>
    <%= f.hidden_field :user_id %>

    <%= f.label :name %>:<br>
    <%= f.text_field :name %><br>
<br>
    <%= f.label :description %>:<br>
    <%= f.text_area :description %><br>
<br>
    <%= f.label :location %>:<br>
    <%= f.text_area :location %><br>
<br>
    <%= f.submit %>
<% end %>